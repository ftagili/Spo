int test_if(int x) {
    if (x > 0) {
        return 1;
    } else {
        return 0;
    }
}

int test_while(int n) {
    int i;
    while (i < n) {
        i;
    }
    return i;
}

int test_dowhile(int n) {
    int i;
    do {
        i;
    } while (i < n);
    return i;
}

int main() {
    int x;
    int y;
    int z;
    test_if(5);
    test_while(10);
    test_dowhile(10);
    return 0;
}

