int printf(string format, ...);
int puts(string s);
void* malloc(int size);

template <T> class List {
    T[] arr;
    int count;
    int capacity;

    void init() {
        arr = new T[3];
        count = 0;
        capacity = 3;
    }

    void expand() {
        capacity = capacity * 2;
        T[] y = new T[capacity];
        int n = 0;
        while (n < count) {
            y[n] = arr[n];
            n = n + 1;
        }
        arr = y;
    }

    void add(T value) {
        if (count >= capacity) {
            expand();
        }
        arr[count] = value;
        count = count + 1;
    }

    void printValues() {
        int n = 0;
        while (n < count) {
            printValue(arr[n]);
            n = n + 1;
        }
    }
}

class Vec2i {
    int x;
    int y;

    void init(int a, int b) {
        x = a;
        y = b;
    }
}

class List_Vec2iPtr {
    Vec2i*[] arr;
    int count;
    int capacity;

    void init() {
        arr = new Vec2i*[3];
        count = 0;
        capacity = 3;
    }

    void expand() {
        capacity = capacity * 2;
        Vec2i*[] y = new Vec2i*[capacity];
        int n = 0;
        while (n < count) {
            y[n] = arr[n];
            n = n + 1;
        }
        arr = y;
    }

    void add(Vec2i* value) {
        if (count >= capacity) {
            expand();
        }
        arr[count] = value;
        count = count + 1;
    }

    void printValues() {
        int n = 0;
        while (n < count) {
            printValue(arr[n]);
            n = n + 1;
        }
    }
}

Vec2i* makeVec2i(int x, int y) {
    Vec2i* v = new Vec2i();
    v->init(x, y);
    return v;
}

void printInt(int x) {
    printf("%d", x);
}

void writeByte(int c) {
    printf("%c", c);
}

void printValue(int n) {
    printInt(n);
    writeByte(10); // '\n'
}

void printValue(Vec2i* v) {
    writeByte('x');
    writeByte('=');
    printInt(v->x);
    writeByte(',');
    writeByte('y');
    writeByte('=');
    printInt(v->y);
    writeByte(10); // '\n'
}

int sum(List<int> a) {
    int n = 0;
    int s = 0;
    while (n < a.count) {
        s = s + a[n];
        n = n + 1;
    }
    return s;
}

Vec2i* sum(List_Vec2iPtr a) {
    int n = 0;
    Vec2i* s = makeVec2i(0, 0);
    while (n < a.count) {
        Vec2i* v = a[n];
        Vec2i* tmp = makeVec2i(s->x + v->x, s->y + v->y);
    s = tmp;
        n = n + 1;
    }
    return s;
}

int main() {
    List<int> vn = new List<int>();
    vn.init();

    int n = 0;
    while (n < 20) {
        vn.add(n);
        n = n + 1;
    }
    vn.printValues();

    List_Vec2iPtr vv = new List_Vec2iPtr();
    vv.init();

    n = 0;
    while (n < 20) {
        vv.add(makeVec2i(n, 20 - n));
        n = n + 1;
    }
    vv.printValues();

    int sum_int = sum(vn);
    printInt(sum_int);
    writeByte(10);

    Vec2i* sum_vec = sum(vv);
    printInt(sum_vec->x);
    writeByte(',');
    printInt(sum_vec->y);
    writeByte(10);

    return 0;
}
