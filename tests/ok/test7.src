template <T> class List {
    T[] arr;
    int count;
    int capacity;
 
    void init() {
        int x[3];
        arr = x;
        count = 0;
        capacity = 3;
    }
    void expand() {
        capacity = capacity * 2;
        int y[capacity];
        int n = 0;
        while (n < count) {
            y[n] = arr[n];
            n = n + 1;
        }
        arr = y;
    }
    void add(T value) {
        if (count >= capacity) {
            expand();
        }
        arr[count] = value;
        count = count + 1;
    }
    void printValues() {
        int n = 0;
        while (n < count) {
            printValue(arr[n]);
            n = n + 1;
        }
    }
}
 
int sum(List<int> a) {
    int n = 0;
    int s = 0; 
    while (n < a.count) {
        s = s + a[n];
    }
    return s;
}
 
class Vec2i {
    int x;
    int y;
 
    void init(int a, int b) {
        x = a;
        y = b;
    }
}
 
Vec2i makeVec2i(int x, int y) {
    Vec2i v;
    v.init(x, y);
    return v;
}
 
 
int sum(List<int> a) {
    int n = 0;
    int s = 0; 
    while (n < a.count) {
        s = s + a[n];
        n = n + 1;
    }
    return s;
}
 
Vec2i sum(List<Vec2i> a) {
    int n = 0;
    Vec2i s = makeVec2i(0, 0);
    while (n < a.count) {
        Vec2i v = a[n];
        s = makeVec2i(s.x + v.x, s.y + v.y);
        n = n + 1;
    }
    return s;
}
 
void printValue_int(int n) {
    printInt(n);
    writeByte(10);
}

void printValue_Vec2i(Vec2i v) {
    writeByte('x');
    writeByte('=');
    printInt(v.x);
    writeByte(10);
    writeByte('y');
    writeByte('='); 
    printInt(v.y);
    writeByte(10);
}
 
int main() {
    List<int> vn;
    vn.init();
 
    int n = 0;
    while (n < 20) {
        vn.add(n);
        n = n + 1;
    }
    vn.printValues();
 
    List<Vec2i> vv;
    vv.init();
 
    n = 0;
    while (n < 20) {
        vv.add(makeVec2i(n, 20 - n));
        n = n + 1;
    }
 
    vv.printValues();
}
