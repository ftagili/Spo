int sumListInt(ListInt a) {
    int n = 0;
    int s = 0; 
    while (n < a.count) {
        s = s + a[n];
        n = n + 1;
    }
    return s;
}
 
class Vec2i {
    int x;
    int y;
 
    void init(int a, int b) {
        x = a;
        y = b;
    }
}
 
Vec2i makeVec2i(int x, int y) {
    Vec2i v;
    v = malloc(24);
    v.init(x, y);
    return v;
}
 
 
int sumInt(ListInt a) { return sumListInt(a); }
 
Vec2i sumListVec2i(ListVec2i a) {
    int n = 0;
    Vec2i s = makeVec2i(0, 0);
    while (n < a.count) {
        Vec2i v = a[n];
        s = makeVec2i(s.x + v.x, s.y + v.y);
        n = n + 1;
    }
    return s;
}

Vec2i sumVec2i(ListVec2i a) { return sumListVec2i(a); }
 
void printValue_int(int n) {
    printInt(n);
    writeByte(10);
}

void printValue_Vec2i(Vec2i v) {
    writeByte('x');
    writeByte('=');
    printInt(v.x);
    writeByte(10);
    writeByte('y');
    writeByte('='); 
    printInt(v.y);
    writeByte(10);
}
 
int main() {
    ListInt vn;
    vn = malloc(32);
    vn.init();
 
    int n = 0;
    while (n < 20) {
        vn.add(n);
        n = n + 1;
    }
    vn.printValues();
 
    ListVec2i vv;
    vv = malloc(32);
    vv.init();
 
    n = 0;
    while (n < 20) {
        vv.add(makeVec2i(n, 20 - n));
        n = n + 1;
    }
 
    vv.printValues();
}





