// Объявления стандартных функций
int printf(string format, ...);
int puts(string s);
void* malloc(int size);

// Шаблонный класс List<T>
template <T> class List {
    T[] arr;
    int count;
    int capacity;

    void init() {
        arr = new T[3];
        count = 0;
        capacity = 3;
    }

    void expand() {
        capacity = capacity * 2;
        T[] y = new T[capacity];
        int n = 0;
        while (n < count) {
            y[n] = arr[n];
            n = n + 1;
        }
        arr = y;
    }

    void add(T value) {
        if (count >= capacity) {
            expand();
        }
        arr[count] = value;
        count = count + 1;
    }

    void printValues() {
        int n = 0;
        while (n < count) {
            printValue(arr[n]);
            n = n + 1;
        }
    }
}

// Класс Vec2i
class Vec2i {
    int x;
    int y;

    void init(int a, int b) {
        x = a;
        y = b;
    }
}

// Конструктор Vec2i
Vec2i makeVec2i(int x, int y) {
    Vec2i v = new Vec2i();
    v.init(x, y);
    return v;
}

// Вспомогательные функции вывода
void printInt(int x) {
    printf("%d", x);
}

void writeByte(int c) {
    printf("%c", c);
}

// Перегруженная функция printValue для int
void printValue(int n) {
    printInt(n);
    writeByte(10); // '\n'
}

// Перегруженная функция printValue для Vec2i
void printValue(Vec2i v) {
    writeByte('x');
    writeByte('=');
    printInt(v.x);
    writeByte(',');
    writeByte('y');
    writeByte('=');
    printInt(v.y);
    writeByte(10); // '\n'
}

// Перегруженная функция sum для List<int>
int sum(List<int> a) {
    int n = 0;
    int s = 0;
    while (n < a.count) {
        s = s + a[n]; // индексация через []
        n = n + 1;
    }
    return s;
}

// Перегруженная функция sum для List<Vec2i>
Vec2i sum(List<Vec2i> a) {
    int n = 0;
    Vec2i s = makeVec2i(0, 0);
    while (n < a.count) {
        Vec2i v = a[n];
        s = makeVec2i(s.x + v.x, s.y + v.y);
        n = n + 1;
    }
    return s;
}

// Основная функция
int main() {
    List<int> vn = new List<int>();
    vn.init();

    int n = 0;
    while (n < 20) {
        vn.add(n);
        n = n + 1;
    }
    vn.printValues();

    List<Vec2i> vv = new List<Vec2i>();
    vv.init();

    n = 0;
    while (n < 20) {
        vv.add(makeVec2i(n, 20 - n));
        n = n + 1;
    }
    vv.printValues();

    // Вывод сумм
    int sum_int = sum(vn);
    printInt(sum_int);
    writeByte(10);

    Vec2i sum_vec = sum(vv);
    printInt(sum_vec.x);
    writeByte(',');
    printInt(sum_vec.y);
    writeByte(10);

    return 0;
}
