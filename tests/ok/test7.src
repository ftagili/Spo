class ListInt {
    int arr;
    int count;
    int capacity;

    void init() {
        arr = malloc(3 * 8);
        count = 0;
        capacity = 3;
    }
    void expand() {
        capacity = capacity * 2;
    int y;
        y = malloc(capacity * 8);
        int n = 0;
        while (n < count) {
            y[n] = arr[n];
            n = n + 1;
        }
        arr = y;
    }
    void add(int value) {
        if (count >= capacity) {
            expand();
        }
        arr[count] = value;
        count = count + 1;
    }
    void printValues() {
        int n = 0;
        while (n < count) {
            printValue_int(arr[n]);
            n = n + 1;
        }
    }
}

